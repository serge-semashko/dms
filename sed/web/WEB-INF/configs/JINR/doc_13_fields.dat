            } 

        }
        else {
            sel_presence[index_Id] = 0;
            div_class = " class='bg_gray'";
            if(sel_ranks[index_Id]==1 || sel_ranks[index_Id]==2 || sel_ranks[index_Id]==3){
                sel_votes[index_Id] = 0;
            }
            if(sel_ranks[index_Id]==4){
                sel_votes[index_Id] = -1;
            }
