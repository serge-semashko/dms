                    rb_checked_3 = "checked";
                }

                votes_onclick_str = " votedatafield = '" + infoFieldId + "' voteindex = '" + index_Id + "'  onclick = 'voteForProtocolNew(this);'";

                div_FOR_str = "<input type=radio class='rb_FOR' name='" + rb_name + "' value='1'" + rb_checked_1 + votes_onclick_str + ">";
