    width=6><img src="#imgPath#filler.gif" width=6 height=1></th> 
</tr></table>

    <script>
        function setType(type)
        { // alert (type);
          if (type=="xp")
          { var f="";
            f = "&form=y"; ??ZZZ&!f_mm&!f_contr
            window.location.href="#ServletPath#?c=nica/list_1&type=xp&svod=g&f_lab=#f_lab#&tt=#tm#" + f; ??!A_LABS
            window.location.href="#ServletPath#?c=nica/list_1&type=xp&f_lab=#f_lab#&tt=#tm#" + f; ??A_LABS
          }
          else 
           if (type=="nal") ??
          { window.location.href="#ServletPath#?c=nica/list_1&type=" + type + "&f_lab=#f_lab#&tt=#tm#";
          }
          else if (type=="mnts") ??
          { window.location.href="#ServletPath#?c=nica/list_1&type=mnts&f_lab=#f_lab#&tt=#tm#"; ??
          } ??
          else ??
          { frm.type.value=type; ??
            frm.submit(); ??
          } ??
        }
    </script>
[end]
