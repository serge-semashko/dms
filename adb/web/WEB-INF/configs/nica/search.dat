[block project]
    $INCLUDE nica/getProjectList.cfg[script] 
    <b>ЛФВЭ, т.1065, &nbsp; Проект:</b>
    <div id="prjPath"></div>
    <div style="border:solid 1px red; margin:0 10px 0 0; text-align:right;	">   ??
    <a href="javascript:resetProject();">сбросить</a> ??
    </div>   ??
    <input type=hidden name="f_sbj" value="1065">
    <input type=hidden name="PROJECT_ID" value="#PROJECT_ID#">
    <iframe name=svsFrame id=svsFrame width=20 height=1 frameborder=no scrolling=no
     src="#ServletPath#?c=nica/getProjectPath&tm=#tm#&prj_id=#PROJECT_ID#&LIST=F&DOC_ID=Y"></iframe>

    <script>
        function setRoot(id)
        {
            document.theForm.PROJECT_ID.value = id;
            var p = document.theForm.PROJECT_ID.value;
            if (p < 0) p = 0;
            openWindow("c=nica/getProjectList&LIST=Y&tm=#tm#&prj_id=" + p,
            "ProjectSelect", 800, 500);
            getProject(); ??
        }
        function resetProject()
        {
            document.theForm.PROJECT_ID.value = "";
            document.getElementById("prjPathTxt").innerHTML="&nbsp;";
        }

        function pasteResults(s, dest)
        {
            document.getElementById(dest).innerHTML=s;
        }
    </script>
[end]


$SET_PARAMETERS f_src_ms1=; ??!M_NICA_VNEB>0

[storeSrcOptions]
$SET_PARAMETERS_SESSION f_src_bud=; f_src_prik=; f_src_ms1=;  ??
$SET_PARAMETERS_SESSION f_src_ms1_=; ??!M_NICA_VNEB>0
$SET_PARAMETERS f_src_bud=#f_src_bud_#; f_src_prik=#f_src_prik_#; f_src_ms1=#f_src_ms1_#;  ??!f_src_ms1&!f_src_ms2&!f_src_prik&!f_src_bud
$SET_PARAMETERS f_src_bud=on; f_src_prik=on; ??!f_src_ms1&!f_src_ms2&!f_src_prik&!f_src_bud
$SET_PARAMETERS_SESSION f_src_bud_=#f_src_bud#; f_src_prik_=#f_src_prik#; f_src_ms1_=#f_src_ms1#; ??!det=y
$SET_PARAMETERS_SESSION f_src_bud_=; f_src_prik_=; f_src_ms1_=; ??det=y
[end]

[srcCheck]
    <tr><td class=l>
        Источники <br>финансирования:</td><td>
        <input type=checkbox name="f_src_bud"
            checked ??f_src_bud
        >бюджет
        <input type=checkbox name="f_src_prik"
            checked ??f_src_prik
        >бюджет-приказы
        <br><input type=checkbox name="f_src_ms1" ??M_NICA_VNEB>0
            checked ??f_src_ms1&M_NICA_VNEB>0
        >Германия ??M_NICA_VNEB>0

        <input type=checkbox name="f_src_ms2" ??M_NICA_VNEB>0
            checked ??f_src_ms2&M_NICA_VNEB>0
        >РФ ??M_NICA_VNEB>0
    </td></tr>

    <tr><td class=l>Конфигурация:</td><td>
        <input type=radio name=f_mega value=""
            checked ??!f_mega
        >все
        <input type=radio name=f_mega value="0"
            checked ??f_mega=0
        >базовая
        <input type=radio name=f_mega value="1"
            checked ??f_mega=1
        >расширенная
        <input type=radio name=f_mega value="null"
            checked ??f_mega=null
        >не опр.
    </td></tr>
[end]


[bank_check]
    $SET_PARAMETERS b_diff=1000  ??!b_diff
    $SET_PARAMETERS_SESSION b_diff=#b_diff#
    <tr><td class=l colspan=2>
    Контроль по банку:
    <input type="radio" name="bank_check" value="" onClick="doIt();"
    checked ??!bank_check
    >Показать все
    <input type="radio" name="bank_check" value="d" onClick="doIt();"
    checked ??bank_check
    >Только с разницей > <input size=4 class=xpc name=b_diff value="#b_diff#"> руб.
    </td></tr>
[end]

