[topTable]
    <table border=0 class="ztable"  width="100%" cellspacing="0" cellpadding="2">
    <tr>
        <td><img src="#imgPath#emblemaLHEP_35.gif" width="63" height="33" border="0" onClick="window.location.href='#ServletPath#?c=nica/i__main&tt=#tm#'"></td>
        <td width="60%" ><h3> NICA</h3></td>
        <td nowrap style="white-space:nowrap; padding-top:10px;">
            Год:&nbsp;
            <select name="f_nica_year" onChange="loadFrame('c=nica/i_setVersions&yr=' + getSelectedVal(this), 'wf2');">
                <option value="2017"
                    selected ??f_nica_year=2017
                >2017
                <option value="2016"
                    selected ??f_nica_year=2016
                >2016
                <option value="2015"
                    selected ??f_nica_year=2015
                >2015
                <option value="2014"
                    selected ??f_nica_year=2014
                >2014
            </select>
        </td>
        <td id="d_versions" nowrap style="white-space:nowrap; padding-top:10px;">
            Версия плана:&nbsp;
            <select name="VER" onChange="refreshTree(0);">
                #VERSIONS#
            </select>
        </td>
        <td class="pt nowrap" nowrap=true style="color:##004080; font-size:9pt; font-weight:bold;" onClick="window.location.href='#ServletPath#'"><img src="#imgPath#home_24.png" width="24" height="24" border="0">На&nbsp;главную</td>
    </tr>
    </table>
[end]


