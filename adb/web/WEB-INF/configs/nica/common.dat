[mega filter]
    $GET_DATA nica/common.dat[check mega]  ??f_mega_
    $SET_PARAMETERS_SESSION f_mega_=#f_mega#; ??f_mega_ZZZ
    $SET_PARAMETERS f_mega=#f_mega_#; ??!f_mega
    $SET_PARAMETERS_SESSION f_mega_=; ??f_mega=ALL&ZZZ
    $SET_PARAMETERS f_mega=; ??f_mega=ALL

<select name="f_mega" onChange="refreshTable();">
    <option value="ALL">все</option>
    <option value=0
    selected ??f_mega=0
    >базовая</option>
    <option value="1"
    selected ??f_mega=1
    >расширенная</option>
    <option value="null"
    selected ??f_mega=null
    >не указана</option>
</select>
[end]

[check mega]
select 'ALL' as "f_mega"
from dual
where '#f_mega_#' not in('0','1','ALL','null')
[end]